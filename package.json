{
  "name": "awesome-mini-pcs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run validate-data && npm run build:data && npm run build:seo && tsc && vite build && npm run copy:seo",
    "build:data": "node scripts/build-data.cjs",
    "build:seo": "npm run build:sitemap && npm run build:stats",
          "build:sitemap": "node scripts/generate-sitemap.cjs",
      "build:stats": "node scripts/generate-stats.cjs",
      "copy:seo": "cp data/stats.json data/sitemap.xml data/robots.txt dist/",
    "validate-data": "node scripts/validate-data.cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@tanstack/react-table": "^8.13.2",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "js-yaml": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "glob": "^10.4.5",
    "lru-cache": "^11.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vite": "^6.2.5"
  }
}
